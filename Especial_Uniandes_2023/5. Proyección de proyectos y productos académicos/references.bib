  title = {Gender-{{Specificity}} of {{Women}}'s and {{Men}}'s {{Self-Reported Attention}} to {{Sexual Stimuli}}},
  author = {Huberman, Jackie S. and Maracle, Amanda C. and Chivers, Meredith L.},
  year = {2015},
