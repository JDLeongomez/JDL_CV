@article{vandermeijTestosteroneCortisolRelease2012,
  title = {Testosterone and {{Cortisol Release}} among {{Spanish Soccer Fans Watching}} the 2010 {{World Cup Final}}},
  author = {{van der Meij}, Leander and Almela, Mercedes and Hidalgo, Vanesa and Villada, Carolina and IJzerman, Hans and {van Lange}, Paul A M and Salvador, Alicia},
