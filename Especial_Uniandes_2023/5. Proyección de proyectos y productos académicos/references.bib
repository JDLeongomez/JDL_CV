  title = {Realistic {{Transformation}} of {{Facial}} and {{Vocal Smiles}} in {{Real-Time Audiovisual Streams}}},
  author = {Arias, Pablo and Soladi{\'e}, Catherine and Bouafif, Oussema and Roebel, Axel and S{\'e}guier, Renaud and Aucouturier, Jean-Julien},
