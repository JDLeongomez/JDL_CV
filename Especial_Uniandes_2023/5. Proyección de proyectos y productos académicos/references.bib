  keywords = {Adult,Choice Behavior,Humans,Male,Odors,Perception,Perfume,Young Adult},
  file = {D:\GDrive\Zotero\PLOS ONE\Lenochová et al_2012_Psychology of fragrance use.pdf}
}

@article{lenochovaMethodsHumanBody2009,
