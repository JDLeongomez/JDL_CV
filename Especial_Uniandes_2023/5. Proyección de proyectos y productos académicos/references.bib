@article{reyesResilienceSocioeconomicStatus2020,
  title = {Resilience and {{Socioeconomic Status}} as {{Predictors}} of {{Life Satisfaction}} and {{Psychological Well-Being}} in {{Colombian Older Adults}}},
  author = {Reyes, Maria-Fernanda and Satorres, Encarnaci{\'o}n and Mel{\'e}ndez, Juan C.},
  year = {2020},
