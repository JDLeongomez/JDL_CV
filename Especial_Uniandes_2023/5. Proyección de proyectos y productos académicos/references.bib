@phdthesis{Leongomez2009,
  title = {Variation of Pitch and Loudness Range of Human Voice in Response to Intra- and Inter-Sexual Stimuli},
  author = {Leong{\'o}mez, Juan David},
  year = {2009},
  number = {September},
  abstract = {The idea of an evolutionary link between music and language has been proposed by several researches since Darwin. Shared neural structures and ontogenetic developmental stages seem to support this theory, but studies addressing specific cognitive similarities are needed. This project investigates the case of two important means for the transmission of information in music: pitch and loudness variation, and their possible roles in speech. In order to see whether variation in these psychoacoustic cues is also important in human language, responses of participants to same- and opposite-sex stimuli with controlled attractiveness were recorded and acoustically analysed, and statistical analyses of the mean, range and standard deviation of the pitch and loudness values of the recordings were conducted. Results show several differences between men and women in the use of these cues, as well as some remarkable similarities that could be the product of inter- and intra-sexual competition strategies to affect positively the perception that listeners would have. The existence of some trends in the use of the of pitch and loudness variation supports the hypothesis that these cues are used to communicate intentions and/or emotions, providing evidence for relations between music and language, analogous to those of the implicated neural resources.},
  copyright = {All rights reserved},
  school = {The University of Liverpool},
  keywords = {\#nosource,Biolinguistics,Dissertation,Intrasexual competition,Loudness variation,Mate choice,MSc Evolutionary Psychology,Origin of music,Pitch variation,Thesis,Vocal attractiveness}
}

@phdthesis{Leongomez2014a,
  title = {Contextual Musicality : Vocal Modulation and Its Perception in Human Social Interaction},
  author = {Leong{\'o}mez, Juan David},
  year = {2014},
  number = {August},
  doi = {10.13140/RG.2.2.36521.39520},
  abstract = {Music and language are both deeply rooted in our biology, but scientists have given far more attention to the neurological, biological and evolutionary roots of language than those of music. Because of this, and probably partially due to this, the purpose of music, in evolutionary terms, remains a mystery. Our brain, physiology and psychology make us capable of producing and listening to music since early infancy; therefore, our biology and behaviour are carrying some of the clues that need to be revealed to understand what music is ``for''. Furthermore, music and language have a deep relationship, particularly in terms of cognitive processing, that can provide clues about the origins of music. Non-verbal behaviours, including voice characteristics during speech, are an important form of communication that enables individual recognition and assessment of the speaker's physical characteristics (including sex, femininity/masculinity, body size, physical strength, and attractiveness). Vocal parameters, however, can be intentionally varied, for example altering the intensity (loudness), rhythm and pitch during speech. This is classically demonstrated in infant directed speech (IDS), in which adults alter vocal characteristics such as pitch, cadence and intonation contours when speaking to infants. In this thesis, I analyse vocal modulation and its perception in human social interaction, in different social contexts such as courtship and authority ranking relationships. Results show that specific vocal modulations, akin to those of IDS, and perhaps music, play a role in communicating courtship intent. Based on these results, as well the body of current knowledge, I then propose a model for the evolution of musicality, the human capacity to process musical information, in relation to human vocal communication. I suggest that musicality may not be limited to specifically musical contexts, and can have a role in other domains such as language, which would provide further support for a common origin of language and music. This model supports the hypothesis of a stage in human evolution in which individuals communicated using a music-like protolanguage, a hypothesis first suggested by Darwin.},
  copyright = {All rights reserved},
  school = {University of Stirling},
  keywords = {Mate choice,Origins of music,Paychoacoustics,Protolanguage,Vocal modulation,Voice pitch},
  file = {D:\GDrive\Zotero\undefined\Leongómez_2014_Contextual musicality.pdf}
}

@article{Leongomez2015,
  title = {La M\'usica Como Objeto de Estudio Cient\'ifico: Consideraciones En Torno a La Musicalidad y El Origen de La M\'usica},
  author = {Leong{\'o}mez, Juan David},
  year = {2015},
  journal = {Pensamiento palabra y obra},
  volume = {13},
  pages = {77--86},
  issn = {2011804X},
  doi = {10.17227/2011804X.15PPO77.86},
  abstract = {La existencia de la m\'usica y su origen contin\'uan siendo un misterio. Sin embargo, la ciencia ha logrado proveer respuestas parciales y modelos te\'oricos, bas\'andose en una variedad de evidencias ha logrado se\~nalar aspectos fundamentales y proponer hip\'otesis que deben ser estudiadas. Este art\'iculo presenta una revisi\'on de la literatura en cuanto al estudio cient\'ifico de la m\'usica, incluyendo el problema central de los universales en m\'usica, la compleja relaci\'on entre m\'usica y lenguaje y las dificultades de los enfoques evolucionistas en torno a la m\'usica. Propone, adem\'as, (1) que la ciencia debe estudiar la musicalidad m\'as que la m\'usica misma, (2) que es esencial tener en cuenta los diferentes aspectos que componen la musicalidad (que podr\'ian tener desarrollos evolutivos independientes), y (3) que es importante dise\~nar experimentos y estudios que permitan separar, tanto los aspectos biol\'ogicos de los culturales, como eventualmente las posibles adaptaciones de las exaptaciones.},
  copyright = {All rights reserved},
  keywords = {m\'usicas,origen de la m\'usica,psicolog\'ia evolucionista,relaci\'on m\'usica-lenguaje},
  file = {D:\GDrive\Zotero\Pensamiento palabra y obra\Leongómez_2015_La música como objeto de estudio científico.pdf}
}

