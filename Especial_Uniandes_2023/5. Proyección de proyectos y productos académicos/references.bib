  author = {Stricker, Reto and Eberhart, Raphael and Chevailler, Marie-Christine and a Quinn, Frank and Bischof, Paul and Stricker, Ren{\'e}},
  year = {2006},
  month = jan,
  journal = {Clinical chemistry and laboratory medicine},
  volume = {44},
  number = {7},
  eprint = {16776638},
