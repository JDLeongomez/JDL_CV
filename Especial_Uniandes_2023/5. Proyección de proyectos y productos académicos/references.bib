@article{whiteheadYouAreWhat2012,
  title = {You Are What You Eat: Within-Subject Increases in Fruit and Vegetable Consumption Confer Beneficial Skin-Color Changes},
  author = {Whitehead, Ross D. and Re, Daniel and Xiao, Dengke and Ozakinci, Gozde and Perrett, David Ian},
