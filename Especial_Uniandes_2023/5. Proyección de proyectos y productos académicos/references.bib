@article{lindhAssessmentEffectEMLA2000,
  title = {Assessment of the Effect of {{EMLA}}\textregistered{} during Venipuncture in the Newborn by Analysis of Heart Rate Variability},
  author = {Lindh, Viveca and Wiklund, Urban and H{\aa}kansson, Stellan},
  year = {2000},
  journal = {Pain},
  volume = {86},
  pages = {247--254},
  issn = {03043959},
  doi = {10.1016/S0304-3959(00)00252-9},
  abstract = {The objective of this study was to investigate the effect of EMLA on the pain response when venipuncture was performed in 60 3-day-old healthy newborns. EMLA/placebo was applied to the back of the baby's hand, following a randomized, double-blind procedure. ECG and crying were recorded during the test. The incidence of crying, heart rate (HR) and spectral analysis of heart rate variability were used to characterize the reaction of the baby to the venipuncture procedure. The occurrence of crying during venipuncture did not differ significantly between the EMLA\textregistered{} and the placebo groups. The placebo- treated group showed a statistically significant higher HR, and a decrease in variance (total power) and power in the low-frequency band (0.02-0.15 Hz) when compared with the EMLA group. We conclude that EMLA decreases the stress response during venipuncture in newborn infants. (C) 2000 International Association for the Study of Pain.},
  isbn = {0304-3959},
  pmid = {10812254},
  keywords = {EMLA,Heart rate variability,Neonate,Pain,Principal component analysis,Venipuncture},
  file = {D:\GDrive\Zotero\Pain\Lindh et al_2000_Assessment of the effect of EMLA® during venipuncture in the newborn by.pdf}
}

