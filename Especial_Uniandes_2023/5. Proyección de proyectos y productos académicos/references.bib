@article{marlerVocalCommunicationDomestic1986,
  title = {Vocal Communication in the Domestic Chicken: {{I}}. {{Does}} a Sender Communicate Information about the Quality of a Food Referent to a Receiver?},
  author = {Marler, Peter and Dufty, Alfred and Pickert, Roberta},
  year = {1986},
