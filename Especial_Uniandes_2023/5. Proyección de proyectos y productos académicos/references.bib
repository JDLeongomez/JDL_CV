@article{castroSintomatologiaAsociadaAgresores2009,
  title = {Sintomatolog\'ia Asociada a Agresores Sexuales En Prisi\'on},
  author = {Castro, Mar{\'i}a Esther and {L{\'o}pez-Castedo}, Antonio and Sueiro, Encarnaci{\'o}n},
