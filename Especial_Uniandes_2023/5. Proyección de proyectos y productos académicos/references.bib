@article{samsonSexualAffectiveResponses2014,
  title = {Sexual and {{Affective Responses}} to {{Same-}} and {{Opposite-Sex Stimuli}} in {{Heterosexual}} and {{Homosexual Men}}: {{Assessment}} and {{Manipulation}} of {{Visual Attention}}},
