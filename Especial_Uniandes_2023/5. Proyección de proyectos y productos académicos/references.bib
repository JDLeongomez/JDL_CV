@article{Fromberger2012,
  ids = {frombergerInitialOrientingSexually2012},
  title = {Initial Orienting towards Sexually Relevant Stimuli: {{Preliminary}} Evidence from Eye Movement Measures},
  author = {Fromberger, Peter and Jordan, Kirsten and Von Herder, Jakob and Steinkrauss, Henrike and Nemetschek, Rebekka and Stolpmann, Georg and {M{\"u} ller}, J{\"u}rgen Leo},
  year = {2012},
  journal = {Archives of Sexual Behavior},
  volume = {41},
  number = {4},
  pages = {919--928},
  issn = {00040002},
  doi = {10.1007/s10508-011-9816-3},
  abstract = {It has been proposed that sexual stimuli will be processed in a comparable manner to other evolutionarily meaningful stimuli (such as spiders or snakes) and therefore elicit an attentional bias and more attentional engagement (Spiering and Everaerd, In E. Janssen (Ed.), The psychophysiology of sex (pp. 166-183). Bloomington: Indiana University Press, 2007). To investigate early and late attentional processes while looking at sexual stimuli, heterosexual men (n = 12) viewed pairs of sexually preferred (images of women) and sexually non-preferred images (images of girls, boys or men), while eye movements were measured. Early attentional processing (initial orienting) was assessed by the number of first fixations and late attentional processing (maintenance of attention) was assessed by relative fixation time. Results showed that relative fixation time was significantly longer for sexually preferred stimuli than for sexually non-preferred stimuli. Furthermore, the first fixation was more often directed towards the preferred sexual stimulus, when simultaneously presented with a non-sexually preferred stimulus. Thus, the current study showed for the first time an attentional bias to sexually relevant stimuli when presented simultaneously with sexually irrelevant pictures. This finding, along with the discovery that heterosexual men maintained their attention to sexually relevant stimuli, highlights the importance of investigating early and late attentional processes while viewing sexual stimuli. Furthermore, the current study showed that sexually relevant stimuli are favored by the human attentional system.},
  isbn = {1573-2800 (Electronic)\textbackslash r0004-0002 (Linking)},
  pmid = {21792688},
  keywords = {\#nosource,Attention,Attentional bias,Eye-Tracking,Gaze pattern,Sexuality}
}

@article{frombergerDiagnosticAccuracyEye2012,
  title = {Diagnostic {{Accuracy}} of {{Eye Movements}} in {{Assessing Pedophilia}}},
  author = {Fromberger, Peter and Jordan, Kirsten and Steinkrauss, Henrike and {von Herder}, Jakob and Witzel, Joachim and Stolpmann, Georg and Kr{\"o}ner-Herwig, Birgit and M{\"u}ller, J{\"u}rgen Leo},
  year = {2012},
  month = jul,
  journal = {The Journal of Sexual Medicine},
  volume = {9},
  number = {7},
  pages = {1868--1882},
  issn = {17436095},
  doi = {10.1111/j.1743-6109.2012.02754.x},
  abstract = {Introduction. Given that recurrent sexual interest in prepubescent\textbackslash nchildren is one of the strongest single predictors for pedosexual\textbackslash noffense recidivism, valid and reliable diagnosis of pedophilia is of\textbackslash nparticular importance. Nevertheless, current assessment methods still\textbackslash nfail to fulfill psychometric quality criteria. Aims. The aim of the\textbackslash nstudy was to evaluate the diagnostic accuracy of eye-movement parameters\textbackslash nin regard to pedophilic sexual preferences. Method. Eye movements were\textbackslash nmeasured while 22 pedophiles (according to ICD-10 F65.4 diagnosis), 8\textbackslash nnon-pedophilic forensic controls, and 52 healthy controls simultaneously\textbackslash nviewed the picture of a child and the picture of an adult. Fixation\textbackslash nlatency was assessed as a parameter for automatic attentional processes\textbackslash nand relative fixation time to account for controlled attentional\textbackslash nprocesses. Main Outcome Measures. Receiver operating characteristic\textbackslash n(ROC) analyses, which are based on calculated age-preference indices,\textbackslash nwere carried out to determine the classifier performance.\textbackslash nCross-validation using the leave-one-out method was used to test the\textbackslash nvalidity of classifiers. Results. Pedophiles showed significantly\textbackslash nshorter fixation latencies and significantly longer relative fixation\textbackslash ntimes for child stimuli than either of the control groups. Classifier\textbackslash nperformance analysis revealed an area under the curve (AUC) = 0.902 for\textbackslash nfixation latency and an AUC = 0.828 for relative fixation time. The\textbackslash neye-tracking method based on fixation latency discriminated between\textbackslash npedophiles and non-pedophiles with a sensitivity of 86.4\% and a\textbackslash nspecificity of 90.0\%. Cross-validation demonstrated good validity of\textbackslash neye-movement parameters. Conclusions. Despite some methodological\textbackslash nlimitations, measuring eye movements seems to be a promising approach to\textbackslash nassess deviant pedophilic interests. Eye movements, which represent\textbackslash nautomatic attentional processes, demonstrated high diagnostic accuracy.\textbackslash nFromberger P, Jordan K, Steinkrauss H, von Herder J, Witzel J, Stolpmann\textbackslash nG, Kroner-Herwig B, and Muller JL. Diagnostic accuracy of eye movements\textbackslash nin assessing pedophilia. J Sex Med 2012;9:18851899.},
  isbn = {17436095 (ISSN)},
  pmid = {22548761},
  keywords = {\#nosource}
}

@article{frombergerEyeMovementsPedophiles2013,
  title = {Eye Movements in Pedophiles: Automatic and Controlled Attentional Processes While Viewing Prepubescent Stimuli.},
  author = {Fromberger, Peter and Jordan, Kirsten and Steinkrauss, Henrike and {von Herder}, Jakob and Stolpmann, Georg and {Kr{\"o}ner-Herwig}, Birgit and M{\"u}ller, J{\"u}rgen Leo},
  year = {2013},
  journal = {Journal of Abnormal Psychology},
  volume = {122},
  number = {2},
  eprint = {23206281},
  eprinttype = {pubmed},
  pages = {587--99},
  issn = {1939-1846},
  doi = {10.1037/a0030659},
  abstract = {Recent theories in sexuality highlight the importance of automatic and controlled attentional processes in viewing sexually relevant stimuli. The model of Spiering and Everaerd (2007) assumes that sexually relevant features of a stimulus are preattentively selected and automatically induce focal attention to these sexually relevant aspects. Whether this assumption proves true for pedophiles is unknown. It is aim of this study to test this assumption empirically for people suffering from pedophilic interests. Twenty-two pedophiles, 8 nonpedophilic forensic controls, and 52 healthy controls simultaneously viewed the picture of a child and the picture of an adult while eye movements were measured. Entry time was assessed as a measure of automatic attentional processes and relative fixation time in order to assess controlled attentional processes. Pedophiles demonstrated significantly shorter entry time to child stimuli than to adult stimuli. The opposite was the case for nonpedophiles, as they showed longer relative fixation time for adult stimuli, and, against all expectations, pedophiles also demonstrated longer relative fixation time for adult stimuli. The results confirmed the hypothesis that pedophiles automatically selected sexually relevant stimuli (children). Contrary to all expectations, this automatic selection did not trigger the focal attention to these sexually relevant pictures. Furthermore, pedophiles were first and longest attracted by faces and pubic regions of children; nonpedophiles were first and longest attracted by faces and breasts of adults. The results demonstrated, for the first time, that the face and pubic region are the most attracting regions in children for pedophiles.},
  pmid = {23206281},
  keywords = {Adult,Age Factors,Analysis of Variance,Attention,Breast,Case-Control Studies,Eye Movement Measurements,Eye Movements,Face,Female,{Fixation, Ocular},{Fixation, Ocular: physiology},Humans,Male,Middle Aged,Pedophilia,Pedophilia: diagnosis,Pedophilia: physiopathology,Pedophilia: psychology,Photic Stimulation,Photic Stimulation: methods,Time Factors},
  file = {D:\GDrive\Zotero\Journal of Abnormal Psychology\Fromberger et al_2013_Eye movements in pedophiles2.pdf}
}

@article{frombergerVirtualViewingTime2015,
  title = {Virtual {{Viewing Time}}: {{The Relationship}} between {{Presence}} and {{Sexual Interest}} in {{Androphilic}} and {{Gynephilic Men}}},
  author = {Fromberger, Peter and Meyer, Sabrina and Kempf, Christina and Jordan, Kirsten and M{\"u}ller, J{\"u}rgen L.},
  editor = {Voracek, Martin},
  year = {2015},
  month = may,
  journal = {PLOS ONE},
  volume = {10},
  number = {5},
  pages = {e0127156},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0127156},
  abstract = {Virtual Reality (VR) has successfully been used in the research of human behavior for more than twenty years. The main advantage of VR is its capability to induce a high sense of presence. This results in emotions and behavior which are very close to those shown in real situations. In the context of sex research, only a few studies have used high-immersive VR so far. The ones that did can be found mostly in the field of forensic psychology. Nevertheless, the relationship between presence and sexual interest still remains unclear. The present study is the first to examine the advantages of high-immersive VR in comparison to a conventional standard desktop system regarding their capability to measure sexual interest. 25 gynephilic and 20 androphilic healthy men underwent three experimental conditions, which differed in their ability to induce a sense of presence. In each condition, participants were asked to rate ten male and ten female virtual human characters regarding their sexual attractiveness. Without their knowledge, the subjects' viewing time was assessed throughout the rating. Subjects were then asked to rate the sense of presence they had experienced as well as their perceived realism of the characters. Results suggested that stereoscopic viewing can significantly enhance the subjective sexual attractiveness of sexually relevant characters. Furthermore, in all three conditions participants looked significantly longer at sexually relevant virtual characters than at sexually non-relevant ones. The high immersion condition provided the best discriminant validity. From a statistical point of view, however, the sense of presence had no significant influence on the discriminant validity of the viewing time task. The study showed that high-immersive virtual environments enhance realism ratings as well as ratings of sexual attractiveness of three-dimensional human stimuli in comparison to standard desktop systems. Results also show that viewing time seems to be influenced neither by sexual attractiveness nor by realism of stimuli. This indicates how important task specific mechanisms of the viewing time effect are.},
  pmid = {25992790},
  keywords = {\#nosource}
}

