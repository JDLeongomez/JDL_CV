@article{coxSystematicReviewBayesian2023,
  title = {A Systematic Review and {{Bayesian}} Meta-Analysis of the Acoustic Features of Infant-Directed Speech},
  author = {Cox, Christopher and Bergmann, Christina and Fowler, Emma and {Keren-Portnoy}, Tamar and Roepstorff, Andreas and Bryant, Greg and Fusaroli, Riccardo},
  year = {2023},
  month = jan,
  journal = {Nature Human Behaviour},
