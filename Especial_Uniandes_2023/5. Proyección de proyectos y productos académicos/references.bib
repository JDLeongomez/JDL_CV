  abstract = {The World Health Organization (WHO) reported that the 1997/98 El Ni\~no might have\textbackslash r\textbackslash nbeen the cause of the dengue fever epidemics in many tropical countries. Because of the interaction\textbackslash r\textbackslash nbetween the atmosphere and the ocean, the warm El Ni\~no and the cold La Ni\~na phases of the El Ni\~no\textbackslash r\textbackslash nSouthern Oscillation (ENSO) engender significant temperature and precipitation anomalies around\textbackslash r\textbackslash nthe world. This paper presents the results of a correlation analysis of past ENSO events with dengue\textbackslash r\textbackslash nepidemics across the Indonesian archipelago and northern South America. Our analysis shows that\textbackslash r\textbackslash nthere is a statistically significant correlation at the 95\% confidence level between El Ni\~no and\textbackslash r\textbackslash ndengue epidemics in French Guiana and Indonesia and at the 90\% confidence level in Colombia and\textbackslash r\textbackslash nSurinam. These regions experience statistically significant warmer temperatures and less rainfall\textbackslash r\textbackslash nduring El Ni\~no years. Public health officials could therefore strongly benefit from El Ni\~no forecasts,\textbackslash r\textbackslash nand they should emphasise control activities such as insecticide sprayings and media campaigns concerning\textbackslash r\textbackslash nthe potential breeding sites of dengue mosquitoes during these years.},
  isbn = {0936-577X},
  keywords = {\#nosource}
}

@article{gagoPedagogiaCrueldadEntrevista2015,
  title = {{La pedagog\'ia de la crueldad: Entrevista a Rita Segato}},
