@article{delgiudiceAdaptiveCalibrationModel2011,
  title = {The {{Adaptive Calibration Model}} of Stress Responsivity},
  author = {Del Giudice, Marco and Ellis, Bruce J. and Shirtcliff, Elizabeth A.},
  year = {2011},
  month = jun,
  journal = {Neuroscience \& Biobehavioral Reviews},
