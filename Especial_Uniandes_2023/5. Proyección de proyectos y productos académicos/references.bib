@article{Ferdenzi2011,
  title = {Digit Ratio ({{2D}}:{{4D}}) Predicts Facial, but Not Voice or Body Odour, Attractiveness in Men.},
  author = {Ferdenzi, Camille and Lema{\^i}tre, Jean-Fran{\c c}ois and Leong{\'o}mez, Juan David and Roberts, S Craig},
