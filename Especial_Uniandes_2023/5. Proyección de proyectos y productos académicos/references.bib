@article{moyanoSexualDesireInventory2017,
  ids = {moyanoSexualDesireInventory2017a},
  title = {Sexual {{Desire Inventory}}: {{Two}} or {{Three Dimensions}}?},
  shorttitle = {Sexual {{Desire Inventory}}},
  author = {Moyano, Nieves and {Vallejo-Medina}, Pablo and Sierra, Juan Carlos},
