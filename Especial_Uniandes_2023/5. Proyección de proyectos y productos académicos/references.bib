@article{chengPridePersonalityEvolutionary2010,
  title = {Pride, Personality, and the Evolutionary Foundations of Human Social Status},
  author = {Cheng, Joey T. and Tracy, Jessica L. and Henrich, Joseph},
  year = {2010},
  month = sep,
  journal = {Evolution and Human Behavior},
  volume = {31},
  number = {5},
  pages = {334--347},
  issn = {10905138},
  doi = {10.1016/j.evolhumbehav.2010.02.004},
  urldate = {2020-06-07},
  abstract = {Based on evolutionary logic, Henrich and Gil-White [Evolution and Human Behavior, 22(3), 165\textendash 196] distinguished between two routes to attaining social status in human societies: dominance, based on intimidation, and prestige, based on the possession of skills or expertise. Independently, emotion researchers Tracy and Robins [Journal of Personality and Social Psychology, 92(3), 506\textendash 525] demonstrated two distinct forms of pride: hubristic and authentic. Bridging these two lines of research, this paper examines whether hubristic and authentic pride, respectively, may be part of the affective-motivational suite of psychological adaptations underpinning the status-obtaining strategies of dominance and prestige. Support for this hypothesis emerged from two studies employing self-reports (Study 1), and self-and peer-reports of group members on collegiate athletic teams (Study 2). Results from both studies showed that hubristic pride is associated with dominance, whereas authentic pride is associated with prestige. Moreover, the two facets of pride are part of a larger suite of distinctive psychological traits uniquely associated with dominance or prestige. Specifically, dominance is positively associated with traits such as narcissism, aggression, and disagreeableness, whereas prestige is positively associated with traits such as genuine self-esteem, agreeableness, conscientiousness, achievement, advice-giving, and prosociality. Discussion focuses on the implications of these findings for our understanding of the evolutionary origins of pride and social status, and the interrelations among emotion, personality, and status attainment. \textcopyright{} 2010 Elsevier Inc. All rights reserved.},
  langid = {english},
  keywords = {archived},
  annotation = {http://web.archive.org/web/20200607225220/https://linkinghub.elsevier.com/retrieve/pii/S1090513810000267},
