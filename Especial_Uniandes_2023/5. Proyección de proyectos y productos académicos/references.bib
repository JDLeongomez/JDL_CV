@article{epelStressBodyShape2000,
  ids = {epelStressBodyShape2000a},
  title = {Stress and {{Body Shape}}: {{Stress-Induced Cortisol Secretion Is Consistently Greater Among Women With Central Fat}}},
  author = {Epel, Elissa S and McEwen, Bruce and Seeman, Teresa and Matthews, Karen and Castellazzo, Grace and Brownell, Kelly D and Bell, Jennifer and Ickovics, Jeannette R},
