@article{robertsHumanOlfactoryCommunication2020,
  title = {Human Olfactory Communication: Current Challenges and Future Prospects},
  shorttitle = {Human Olfactory Communication},
  author = {Roberts, S. Craig and Havl{\'i}{\v c}ek, Jan and Schaal, Benoist},
  year = {2020},
