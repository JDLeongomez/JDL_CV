@article{sternVoicesCarryValid2021,
  title = {Do Voices Carry Valid Information about a Speaker's Personality?},
  author = {Stern, Julia and Schild, Christoph and Jones, Benedict C. and DeBruine, Lisa M. and Hahn, Amanda and Puts, David A. and Zettler, Ingo and Kordsmeyer, Tobias L. and Feinberg, David and Zamfir, Dan and Penke, Lars and Arslan, Ruben C.},
