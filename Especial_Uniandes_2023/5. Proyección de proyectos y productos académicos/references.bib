@article{leipoldAbsoluteRelativePitch2019,
  title = {Absolute and Relative Pitch Processing in the Human Brain: Neural and Behavioral Evidence},
  shorttitle = {Absolute and Relative Pitch Processing in the Human Brain},
  author = {Leipold, Simon and Brauchli, Christian and Greber, Marielle and J{\"a}ncke, Lutz},
