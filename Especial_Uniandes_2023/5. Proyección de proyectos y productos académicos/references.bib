  title = {The Genome of the Offspring of a {{Neanderthal}} Mother and a {{Denisovan}} Father},
  author = {Slon, Viviane and Mafessoni, Fabrizio and Vernot, Benjamin and {de Filippo}, Cesare and Grote, Steffi and Viola, Bence and Hajdinjak, Mateja and Peyr{\'e}gne, St{\'e}phane and Nagel, Sarah and Brown, Samantha and Douka, Katerina and Higham, Tom and Kozlikin, Maxim B. and Shunkov, Michael V. and Derevianko, Anatoly P. and Kelso, Janet and Meyer, Matthias and Pr{\"u}fer, Kay and P{\"a}{\"a}bo, Svante},
