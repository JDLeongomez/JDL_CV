@incollection{fitchBiologyEvolutionRhythm2011a,
  ids = {fitchBiologyEvolutionRhythm2011},
  title = {The Biology and Evolution of Rhythm: Unravelling a Paradox},
  booktitle = {Language and Music as Cognitive Systems},
  author = {Fitch, W Tecumseh},
  editor = {Rebuschat, Patrick and Rohmeier, Martin and Hawkins, John A and Cross, Ian},
  year = {2011},
