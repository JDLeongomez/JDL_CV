  pmid = {17628754},
  keywords = {Choice Behavior,Female,Genetic,Genetic Variation,Heterozygote,HLA Antigens,HLA Antigens: metabolism,Humans,Major Histocompatibility Complex,Male,Odors,Selection,Sex Characteristics,Sexual Behavior,Sexual Behavior: physiology},
