@article{dochertyMoreAttractiveWomen2020,
  title = {Do More Attractive Women Show Stronger Preferences for Male Facial Masculinity?},
  author = {Docherty, Ciaran and Lee, Anthony J. and Hahn, Amanda C. and DeBruine, Lisa M. and Jones, Benedict C.},
  year = {2020},
  month = jul,
  journal = {Evolution and Human Behavior},
  volume = {41},
  number = {4},
  pages = {312--317},
  issn = {10905138},
  doi = {10.1016/j.evolhumbehav.2020.05.005},
  urldate = {2020-06-07},
  abstract = {Researchers have suggested that more attractive women will show stronger preferences for masculine men because such women are better placed to offset the potential costs of choosing a masculine mate. However, evidence for correlations between measures of women's own attractiveness and preferences for masculine men is mixed. Moreover, the samples used to test this hypothesis are typically relatively small. Consequently, we conducted two large-scale studies that investigated possible associations between women's preferences for facial masculinity and their own attractiveness as assessed from third-party ratings of their facial attractiveness (Study 1, N = 454, laboratory study) and self-rated attractiveness (Study 2, N = 8972, online study). Own attractiveness was positively correlated with preferences for masculine men in Study 2 (self-rated attractiveness), but not Study 1 (thirdparty ratings of facial attractiveness). This pattern of results is consistent with the proposal that women's beliefs about their own attractiveness, rather than their physical condition per se, underpins attractiveness-contingent masculinity preferences.},
  langid = {english},
  keywords = {archived},
  annotation = {http://web.archive.org/web/20200607225227/https://linkinghub.elsevier.com/retrieve/pii/S1090513820300611},
  file = {D:\GDrive\Zotero\Evolution and Human Behavior\Docherty et al_2020_Do more attractive women show stronger preferences for male facial masculinity2.pdf}
}

@article{dolezalChildhoodSexualExperiences2002,
