  keywords = {Adolescent,Adult,Analysis of Variance,Circadian Rhythm,Fingers,Fingers: innervation,Humans,Male,Psychomotor Performance,Psychomotor Performance: physiology,Saliva,Saliva: metabolism,Speech Acoustics,Testosterone,Testosterone: metabolism,Voice,Voice Quality,Voice Quality: physiology},
