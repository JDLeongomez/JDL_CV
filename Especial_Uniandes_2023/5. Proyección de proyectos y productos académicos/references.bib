@article{ondrusekRiskScreeningTool2015,
  title = {A Risk Screening Tool for Ethical Appraisal of Evidence-Generating Initiatives},
  author = {Ondrusek, Nancy K. and Willison, Donald J. and Haroun, Vinita and Bell, Jennifer A. H. and Bornbaum, Catherine C.},
  year = {2015},
  month = jul,
  journal = {BMC Medical Ethics},
  volume = {16},
  number = {1},
  pages = {47},
  issn = {1472-6939},
  doi = {10.1186/s12910-015-0039-3},
  urldate = {2020-08-06},
  abstract = {The boundaries between health-related research and practice have become blurred as initiatives traditionally considered~to be practice (e.g., quality improvement, program evaluation) increasingly use the same methodology as research. Further, the application of different ethical requirements based on this distinction raises concerns because many initiatives commonly labelled as ``non-research'' are associated with risks to patients, participants, and other stakeholders, yet may not be subject to any ethical oversight. Accordingly, we sought to develop a tool to facilitate the systematic identification of risks to human participants and determination of risk level across a broad range of projects (e.g., clinical research, laboratory-based projects, population-based surveillance, and program evaluation) and health-related contexts. This paper describes the development of the Public Health Ontario (PHO) Risk Screening Tool.},
  annotation = {http://web.archive.org/web/20200806212948/https://bmcmedethics.biomedcentral.com/articles/10.1186/s12910-015-0039-3},
