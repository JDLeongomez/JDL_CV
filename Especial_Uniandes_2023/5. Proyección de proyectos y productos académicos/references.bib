@article{sanchez-quintoGutMicrobiomeChildren2020,
  title = {Gut {{Microbiome}} in {{Children}} from {{Indigenous}} and {{Urban Communities}} in {{M\'exico}}: {{Different Subsistence Models}}, {{Different Microbiomes}}},
  shorttitle = {Gut {{Microbiome}} in {{Children}} from {{Indigenous}} and {{Urban Communities}} in {{M\'exico}}},
  author = {{S{\'a}nchez-Quinto}, Andr{\'e}s and {Cerqueda-Garc{\'i}a}, Daniel and Falc{\'o}n, Luisa I. and Gaona, Osiris and {Mart{\'i}nez-Correa}, Santiago and Nieto, Javier and {G-Santoyo}, Isaac},
