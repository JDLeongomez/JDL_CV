  keywords = {\#nosource,infant development,memory,music,open data,open materials,social cognition}
}

@article{mehrFormFunctionHuman2018,
  ids = {mehrFormFunctionHuman2018a},
  title = {Form and {{Function}} in {{Human Song}}},
  author = {Mehr, Samuel A. and Singh, Manvir and York, Hunter and Glowacki, Luke and Krasnow, Max M.},
  year = {2018},
  month = feb,
  journal = {Current Biology},
  volume = {28},
