@article{schifferFunctionalBrainCorrelates2008,
  title = {Functional Brain Correlates of Heterosexual Paedophilia},
  author = {Schiffer, Boris and Paul, Thomas and Gizewski, Elke and Forsting, Michael and Leygraf, Norbert and Schedlowski, Manfred and Kruger, Tillmann H C},
  year = {2008},
