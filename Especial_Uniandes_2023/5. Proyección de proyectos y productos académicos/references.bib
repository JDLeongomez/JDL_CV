@article{ravignaniRhythmSpeechAnimal2019,
  title = {Rhythm in Speech and Animal Vocalizations: A Cross-Species Perspective},
  shorttitle = {Rhythm in Speech and Animal Vocalizations},
  author = {Ravignani, Andrea and Bella, Simone Dalla and Falk, Simone and Kello, Christopher T. and Noriega, Florencia and Kotz, Sonja A.},
