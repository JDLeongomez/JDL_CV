@article{robertsPartnerChoiceRelationship2014,
  title = {Partner {{Choice}}, {{Relationship Satisfaction}}, and {{Oral Contraception}}: {{The Congruency Hypothesis}}.},
  author = {Roberts, S Craig and Little, Anthony C and Burriss, Robert P. and Cobey, Kelly D and Klapilov{\'a}, Kate{\v r}ina and Havl{\'i}{\v c}ek, Jan and Jones, Benedict C. and DeBruine, Lisa and Petrie, Marion},
  year = {2014},
