  langid = {english},
  pmid = {27791090},
  keywords = {culture,disgust,evolutionary psychology,pathogens,political ideology},
  file = {D\:\\GDrive\\Zotero\\Proceedings of the National Academy of Sciences\\Tybur et al_2016_Parasite stress and pathogen avoidance relate to distinct dimensions of.pdf;D\:\\Zotero Library\\storage\\PHLG54XE\\12408.html}
