  keywords = {\#nosource}
}

@article{helwegComparisonSongsHumpback1990,
  title = {Comparison of {{Songs}} of {{Humpback Whales}} ({{Megaptera Novaeangliae}}) Recorded in {{Japan}}, {{Hawaii}}, and {{Mexico During}} the {{Winter}} of 1989},
  author = {Helweg, D A and Herman, L M and Yamamoto, S and Forestell, P H},
  year = {1990},
  journal = {Scientific Reports of the Cetacean Research Institute},
  volume = {1},
  pages = {1--20},
  abstract = {Recordings of humpback whale (Megaptera novaeangliae) song were obtained between 10 March and 14 April 1989 from the Bonin Islands and the Ryukyuan Islands of Japan, the Hawaiian Islands (USA), and the southeastern Baja Peninsula of Mexico. Through analyses of the song samples obtained, seven "themes" were identified in Hawaiian and in Bonin song and six in Mexican song. Theme similarities across these three regions arranged themselves hierarchically: Hawaii and Mexico shared five themes in common, and did not share three; Hawaii and Bonin shared four themes and did not share three and Mexico and Bonin shared three themes and did not share six. Three themes appeared in all three regions. The song samples of Ryukyu were all of short duration and were likely an incomplete record of the entire song repertoire. Nevertheless, of the three themes identified in the Ryukyuan samples, two appeared in Bonin song and none in Mexican and Hawaiian song. Overall, the thematic data suggested that there is acoustic contact during some portion of the migratory cycle among the whales wintering in Mexico, Hawaii, and the Bonin Islands.},
  keywords = {\#nosource,⛔ No DOI found}
}

