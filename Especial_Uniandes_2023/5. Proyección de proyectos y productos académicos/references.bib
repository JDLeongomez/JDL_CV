@article{Salska2008,
  title = {Conditional Mate Preferences: {{Factors}} Influencing Preferences for Height},
  author = {Salska, Irmina and Frederick, David A. and Pawlowski, Boguslaw and Reilly, Andrew H. and Laird, Kelsey T. and Rudd, Nancy A.},
