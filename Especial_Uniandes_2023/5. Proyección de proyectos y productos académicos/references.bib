@article{schifferStructuralBrainAbnormalities2007,
  title = {Structural Brain Abnormalities in the Frontostriatal System and Cerebellum in Pedophilia},
  author = {Schiffer, Boris and Peschel, Thomas and Paul, Thomas and Gizewski, Elke and Forsting, Michael and Leygraf, Norbert and Schedlowski, Manfred and Krueger, Tillmann H C},
