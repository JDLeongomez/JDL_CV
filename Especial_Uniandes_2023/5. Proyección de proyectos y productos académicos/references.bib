@article{Paajanen2010,
  title = {Short Stature Is Associated with Coronary Heart Disease: A Systematic Review of the Literature and a Meta-Analysis},
  author = {Paajanen, Tuula A. and Oksala, Niku K.J. and Kuukasj{\"a}rvi, Pekka and Karhunen, Pekka J.},
  year = {2010},
  month = jul,
  journal = {European Heart Journal},
  volume = {31},
  number = {14},
  pages = {1802--1809},
  issn = {0195-668X},
  doi = {10.1093/eurheartj/ehq155},
  abstract = {The aim of this study was to assess the relationship between short stature and coronary heart disease (CHD) morbidity and mortality. We performed a systematic search from MEDLINE, PREMEDLINE, and All EBM Reviews as well as from a reference list of relevant articles. We used SPICO (Study design, Patient, Intervention, Control-intervention, Outcome) criteria. The methodological quality of studies was analysed by modified Borghoust criteria. From a total of 1907 articles, we selected 52 studies comprising population-based follow-up studies and patient cohorts followed after a CHD event, as well as case-control studies with height either as a continuous or categorical variable, totalling 3 012 747 individuals. The short ones were below 160.5 cm and tall ones over 173.9 cm on average. Among the shortest height category, the relative risks were 1.35 (95\% CI 1.25,\"A\`i1.44) for all-cause mortality, 1.55 (1.37,\"A\`i1.74) for all cardiovascular disease (CVD) mortality, 1.49 (1.33,\"A\`i1.67) for CHD, and 1.52 (1.28,\"A\`i1.81) for myocardial infarction when compared with those within the highest height category. The mean relative risk was 1.46 (1.37,\"A\`i1.55). Short stature was associated with increased cardiovascular morbidity and mortality in both genders. The relationship between short stature and CVD appears to be a real one. On the basis of comparison, adults within the shortest category had an ,\`a\textordmasculine 50\% higher risk of CHD morbidity and mortality than tall individuals.},
  isbn = {1522-9645},
  pmid = {20530501},
  keywords = {\#nosource}
}

