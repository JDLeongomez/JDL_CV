@article{rosenfieldSexualSelectionLow2020,
  title = {Sexual Selection for Low Male Voice Pitch among {{Amazonian}} Forager-Horticulturists},
  author = {Rosenfield, Kevin A. and Sorokowska, Agnieszka and Sorokowski, Piotr and Puts, David A.},
