  author = {Law Smith, M.J and Perrett, David Ian and Jones, Benedict C. and Cornwell, R Elisabeth and Moore, F.R and Feinberg, David R and Boothroyd, Lynda G and Durrani, S.J and Stirrat, M.R and Whiten, S and Pitman, R.M and Hillier, S.G},
