@article{gonzalez-villarWhenBrainSimulates2016,
  title = {When the Brain Simulates Stopping: {{Neural}} Activity Recorded during Real and Imagined Stop-Signal Tasks},
  shorttitle = {When the Brain Simulates Stopping},
  author = {{Gonz{\'a}lez-Villar}, Alberto J. and Bonilla, F. Mauricio and {Carrillo-de-la-Pe{\~n}a}, Mar{\'i}a T.},
  year = {2016},
