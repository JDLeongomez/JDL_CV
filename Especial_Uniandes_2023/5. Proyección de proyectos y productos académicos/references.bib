@article{pardoDesarrolloCuestionarioPara2004,
  title = {Desarrollo de Un Cuestionario Para La Valoraci\'on y Cuantificaci\'on de Los H\'abitos de Vida Relacionados Con El Sobrepeso y La Obesidad},
  author = {Pardo, A and Ruiz, M and Jodar, E and Garrido, J and {de Rosendo}, J M and Usan, L A},
