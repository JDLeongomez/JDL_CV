@article{tanDailyConsumptionFruit2015,
  title = {Daily Consumption of a Fruit and Vegetable Smoothie Alters Facial Skin Color},
  author = {Tan, Kok Wei and Graf, Brigitte A. and Mitra, Soma R. and Stephen, Ian D.},
