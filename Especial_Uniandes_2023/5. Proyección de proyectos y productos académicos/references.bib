@incollection{bradshawFecundity2008,
  title = {Fecundity},
  booktitle = {Encyclopedia of {{Ecology}}},
  author = {Bradshaw, C. J. A. and McMahon, C. R.},
  editor = {Fath, Brian},
  year = {2008},
