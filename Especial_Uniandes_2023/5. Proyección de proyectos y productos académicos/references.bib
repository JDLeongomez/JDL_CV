@article{dipietroReceptiveAmusiaTemporal2004,
  title = {Receptive Amusia: Temporal Auditory Processing Deficit in a Professional Musician Following a Left Temporo-Parietal Lesion.},
  author = {Di Pietro, Marie and Laganaro, Marina and Leemann, B{\'e}atrice and Schnider, Armin},
  year = {2004},
  month = jan,
  journal = {Neuropsychologia},
  volume = {42},
  number = {7},
  eprint = {14998702},
  eprinttype = {pubmed},
  pages = {868--877},
  issn = {0028-3932},
  doi = {10.1016/j.neuropsychologia.2003.12.004},
  urldate = {2012-03-14},
  abstract = {This study examined the musical processing in a professional musician who suffered from amusia after a left temporo-parietal stroke. The patient showed preserved metric judgement and normal performance in all aspects of melodic processing. By contrast, he lost the ability to discriminate or reproduce rhythms. Arrhythmia was only observed in the auditory modality: discrimination of auditorily presented rhythms was severely impaired, whereas performance was normal in the visual modality. Moreover, a length effect was observed in discrimination of rhythm, while this was not the case for melody discrimination. The arrhythmia could not be explained by low-level auditory processing impairments such as interval and length discrimination and the impairment was limited to auditory input, since the patient produced correct rhythmic patterns from a musical score. Since rhythm processing was selectively disturbed in the auditory modality, the arrhythmia cannot be attributed to a impairment of supra-modal temporal processing. Rather, our findings suggest modality-specific encoding of musical temporal information. Besides, it is proposed that the processing of auditory rhythmic sequences involves a specific left hemispheric temporal buffer.},
  pmid = {14998702},
  keywords = {\#nosource,Acoustic Stimulation,Acoustic Stimulation: methods,Adult,Auditory Perception,Auditory Perception: physiology,Auditory Perceptual Disorders,Auditory Perceptual Disorders: etiology,Brain Injuries,Brain Injuries: complications,Functional Laterality,Functional Laterality: physiology,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Memory,Memory: physiology,Mental Processes,Mental Processes: physiology,Middle Aged,Music,Neuropsychological Tests,Parietal Lobe,Parietal Lobe: physiopathology,Pitch Discrimination,Pitch Discrimination: physiology,Psychomotor Performance,Psychomotor Performance: physiology,Temporal Lobe,Temporal Lobe: physiopathology}
}

