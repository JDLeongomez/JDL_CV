  booktitle = {Homo {{Novus}}: {{A Human Without Illusions}}},
  author = {Sear, Rebecca},
  editor = {Frey, Ulrich J. and St{\"o}rmer, Charlotte and Willf{\"u}hr, Kai P.},
  year = {2010},
  volume = {44},
