@incollection{grammerFemaleFacesBodies2002,
  title = {Female Faces and Bodies: {{N-dimensional}} Feature Space and Attractiveness},
  booktitle = {Advances in Visual Cognition, {{Vol}}. 1. {{Facial}} Attrativeness: {{Evolutionary}}, Cognitive, and Social Perspectives},
