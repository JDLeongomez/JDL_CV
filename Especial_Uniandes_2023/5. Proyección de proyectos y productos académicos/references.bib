  author = {Pakarinen, Satu and Teinonen, Tuomas and Shestakova, Anna and Kwon, Myoung Soo and Kujala, Teija and H{\"a}m{\"a}l{\"a}inen, Heikki and N{\"a}{\"a}t{\"a}nen, Risto and Huotilainen, Minna},
