@article{jonesEffectsMenstrualCycle2008,
  title = {Effects of Menstrual Cycle Phase on Face Preferences},
  author = {Jones, Benedict C and DeBruine, Lisa M and Perrett, David Ian and Little, Anthony C and Feinberg, David R and Law Smith, Miriam J},
  year = {2008},
  month = feb,
  journal = {Archives of Sexual Behavior},
  volume = {37},
