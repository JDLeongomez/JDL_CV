  doi = {10.1023/B:ASEB.0000037426.55935.9c},
  abstract = {The Screening Scale for Pedophilic Interests (SSPI; Seto \& Lalumi\`ere, 2001), a brief measure of sexual attraction to prepubescent children that is based on victim characteristics, was used in two samples of 113 and 145 adult male sex offenders with child victims. In both samples, the SSPI was significantly and positively correlated with an index of phallometrically-measured sexual arousal to stimuli depicting prepubescent children. It was also significantly and positively correlated in both samples with violent recidivism (meaning either nonsexually violent offenses or sexual offenses involving physical contact with a victim), and positively correlated with sexual recidivism, significantly so in the second, larger sample. Focusing on the larger sample, the SSPI added to the predictive accuracy of a measure of general antisociality (the Psychopathy Checklist-Revised), while the phallometric index of sexual arousal did not add predictive accuracy once the other two measures were entered. The SSPI also yielded the same interaction between anomalous sexual interests and psychopathy we had previously reported using phallometric data (Rice \& Harris, 1997). These findings suggest that the SSPI has predictive utility among adult male sex offenders with child victims, and accounts for variance in sexual offending that is not explained by phallometric testing.},
  isbn = {0004-0002 (Print)\textbackslash n0004-0002 (Linking)},
  pmid = {15305116},
  keywords = {\#nosource}
}

@article{SexDifferencesEvaluation2013,
  title = {Sex Differences in the Evaluation of and Communication with Conversational Agents},
  year = {2013},
  journal = {Human Ethology Bulletin},
  abstract = {We investigate dialogue data from users' chats with artificial communication systems. The communication context is comparable to a bar scenario with an artificial bartender and a human client. The data stem from two dif- ferent experimental settings. In the one, the users communicate with three agents in different mood, displaying positive, negative and, as control con- dition, neutral communication attitude. In the other one, the agents vary in their simulations of social talk, i.e. getting acquainted with the user versus sharing an emotionally rich story with the user versus no social talk as con- trol condition. We are particularly interested in evidence for or against sex differences in the evaluation of the agents and in language use represented by occurrence of dialogue acts, LIWC categories, emoticons and length of dialogue contribution. The results indicate that differences between male and female users are largely emotion related. While male participants of the study were more talkative with the agent in positive mood, they also took on the agent in negative mood by making significantly more statements in the conversation with the agent than females did. In contrast, females were put off by this agent (used more rejections, negations, and no-answers), but reacted more positively and more affectively than males did to the agents displaying social talk, whereas topic-oriented talk was prevailing in male dialogue contributions to these agents.},
  keywords = {\#nosource,affect,conversational agents,empirical studies,Revisi\'on}
}

