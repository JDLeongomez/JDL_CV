@article{christodoulouValidationBriefYesterday2014,
  title = {Validation of a {{Brief Yesterday Measure}} of {{Hedonic Well-Being}} and {{Daily Activities}}: {{Comparison}} with the {{Day Reconstruction Method}}.},
  author = {Christodoulou, Christopher and Schneider, Stefan and a Stone, Arthur},
