@article{kalpidouRelationshipFacebookWellbeing2011,
  title = {The Relationship between {{Facebook}} and the Well-Being of Undergraduate College Students.},
  author = {Kalpidou, Maria and Costin, Dan and Morris, Jessica},
  year = {2011},
