@article{koenigDukeUniversityReligion2010,
  title = {The {{Duke University Religion Index}} ({{DUREL}}): {{A Five-Item Measure}} for {{Use}} in {{Epidemological Studies}}},
  author = {Koenig, Harold G. and B{\"u}ssing, Arndt},
  year = {2010},
