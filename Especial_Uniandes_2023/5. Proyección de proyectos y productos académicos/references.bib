@article{preacherAsymptoticResamplingStrategies2008,
  ids = {preacherAsymptoticResamplingStrategies2008a},
  title = {Asymptotic and Resampling Strategies for Assessing and Comparing Indirect Effects in Multiple Mediator Models},
  author = {Preacher, Kristopher J. and Hayes, Andrew F.},
