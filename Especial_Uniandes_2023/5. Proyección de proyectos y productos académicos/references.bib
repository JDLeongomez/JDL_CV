@article{shiraziFilipinoWomenPreferences2018,
  title = {Filipino {{Women}}'s {{Preferences}} for {{Male Voice Pitch}}: {{Intra-Individual}}, {{Life History}}, and {{Hormonal Predictors}}},
  shorttitle = {Filipino {{Women}}'s {{Preferences}} for {{Male Voice Pitch}}},
  author = {Shirazi, Talia N. and Puts, David A. and {Escasa-Dorne}, Michelle J.},
