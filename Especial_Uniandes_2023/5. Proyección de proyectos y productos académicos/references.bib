@article{frowdRelativeImportanceExternal2007,
  title = {The Relative Importance of External and Internal Features of Facial Composites},
  author = {Frowd, Charlie and Bruce, Vicki and McIntyre, Alex and Hancock, Peter},
  year = {2007},
